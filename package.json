    "@pagefind/default-ui": "^1.4.0",
    "astro": "^5.15.9",
    "astro-expressive-code": "^0.41.3",
    "astro-pagefind": "^1.8.5",
    "pagefind": "^1.4.0"
  },
  "devDependencies": {
    "@astrojs/mdx": "^4.3.11",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
